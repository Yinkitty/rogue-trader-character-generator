<%= semantic_form_for @character do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :career_path_id %><br />
    <%= f.text_field :career_path_id %>
  </p>
  <p>
    <%= f.label :experience_to_spend %><br />
    <%= f.text_field :experience_to_spend %>
  </p>
  <p>
    <%= f.label :experience_spent %><br />
    <%= f.text_field :experience_spent %>
  </p>
  <p><%= f.submit %></p>
<% end %>
